})

local Number = game:GetService("Players").LocalPlayer.leaderstats.Stage.Value 
 
for i,v in pairs(game.Workspace:GetChildren()) do 
 Number = Number + 1 
 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace[Number].CFrame 
 wait(1) 
end
